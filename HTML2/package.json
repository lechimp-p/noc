{
  "name": "NoC-frontend",
  "description": "The most popular front-end framework for developing responsive, mobile first projects on the web.",
  "version": "0.1.0",
  "engines": {
    "node": "~0.10.1"
  },
  "jspm": {
    "main": "js/bootstrap",
    "directories": {
      "example": "examples",
      "lib": "dist"
    },
    "shim": {
      "js/bootstrap": {
        "imports": "jquery",
        "exports": "$"
      }
    },
    "buildConfig": {
      "uglify": true
    }
  },
  "devDependencies": {
    "grunt": "~0.4.5",
    "grunt-contrib-clean": "~0.5.0"
  }
}
